# Telegram Configuration
TELEGRAM_TOKEN=your_telegram_bot_token
CHAT_ID=your_chat_id

# Backup Configuration
BACKUP_DIR=/mnt/c/Users/YourUsername/backup-server/backups
BACKUP_SOURCE=/mnt/c/Users/YourUsername/Documents
KEEP_BACKUPS=3

# Additional excludes (comma-separated)
ADDITIONAL_EXCLUDES=*.bak,*.tmp,node_modules,.git

# Logging
LOG_LEVEL=INFO
LOG_DIR=logs
